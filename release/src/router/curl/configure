squeeze() {
  _sqz_result=""
  eval _sqz_input=\$$1
  for _sqz_token in $_sqz_input; do
    if test -z "$_sqz_result"; then
      _sqz_result="$_sqz_token"
    else
      _sqz_result="$_sqz_result $_sqz_token"
    fi
  done
  eval $1=\$_sqz_result
  return 0
}


